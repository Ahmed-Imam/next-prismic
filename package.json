{
  "name": "skyward-starter-next-prismic",
  "version": "0.1",
  "description": "An awesome website made with Prismic (slicemachines), NextJS and Tailwind",
  "author": "<skyward-digital>",
  "license": "MIT",
  "main": "slices/index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "prepublishOnly": "node ./node_modules/sm-commons/scripts/bundle",
    "postbuild": "next-sitemap",
    "sm": "prismic sm --develop",
    "sm:create": "prismic sm --create-slice",
    "storybook": "start-storybook -p 8888",
    "build-storybook": "build-storybook",
    "slicemachine": "start-slicemachine --port 9999",
    "format": "prettier --write \"{.slicemachine,.storybook,components,context,customtypes,docs,pages,slices,styles,utils}/**/*.{js,jsx,ts,tsx,css,scss,json,yaml,md}\"",
    "format:check": "prettier --check '{.slicemachine,.storybook,components,context,,customtypes,docs,pages,slices,styles,utils}/**/*.{js,jsx,ts,tsx,css,scss,json,yaml,md}'",
    "lint": "eslint --fix '{components,context,pages,slices,utils}/**/*.js'",
    "lint:check": "eslint '{components,context,pages,slices,utils}/**/*.js'",
    "cypress": "cypress open",
    "cy:run": "cypress run",
    "kill": "kill $(lsof -ti:3000,8888,9999)"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@headlessui/react": "^1.4.1",
    "@prismicio/client": "^6.0.0",
    "@prismicio/helpers": "^2.0.0",
    "@prismicio/react": "^2.0.3-debug.0",
    "@tailwindcss/line-clamp": "^0.2.2",
    "@tailwindcss/typography": "^0.4.1",
    "next": "^11.1.2",
    "next-sitemap": "^1.6.157",
    "next-slicezone": "^0.1.2",
    "next-transpile-modules": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.17.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.6",
    "@storybook/testing-react": "^0.0.22",
    "autoprefixer": "^10.2.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-react-require": "^3.1.3",
    "chromatic": "^6.0.3",
    "cypress": "^8.2.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^11.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-tailwind": "^0.2.0",
    "postcss": "^8.3.5",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.5",
    "prettier": "^2.3.2",
    "prismic-cli": "^4.2.0",
    "shx": "^0.3.3",
    "slice-machine-ui": "^0.1.2",
    "sm-commons": "^0.0.24",
    "tailwindcss": "^2.2.7"
  }
}
